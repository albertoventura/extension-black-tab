{
    "manifest_version": 3,
    "name": "Black Tab Extension",
    "version": "1.0",
    "description": "Cria uma nova aba com fundo preto",
    
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],
    "web_accessible_resources": [{
      "resources": ["black-tab.html"],
      "matches": ["<all_urls>"]
    }],
    "background": {
      "service_worker": "black-tab.js"
    },
    "action": { }
  }
  